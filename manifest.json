{
  "manifest_version": 2,
  "name": "Irish Times Unblocker",
  "version": "1.0",
  "browser_action": {
    "default_icon": "shamrock.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "tabs",
    "browsingData"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.irishtimes.com/*"
      ],
      "js": ["content.js"]
    }
  ]
}
